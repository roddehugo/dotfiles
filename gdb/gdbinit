set history save
set history filename ~/.gdb_history
set confirm off
set mem inaccessible-by-default off
define scan
 monitor swdp_scan
 attach 1
end
define bmp
 target extended-remote /dev/cu.usbmodemD5D884E81
end
define stlink
 target extended-remote :4242
end
define split
 layout src
 focus cmd
end
